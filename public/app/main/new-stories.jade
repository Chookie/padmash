.panel.panel-primary
   .panel-heading.text.center New Stories
   .panel-body
      .row(ng-repeat="story in stories | orderBy: 'published':true | limitTo: 10")
         .col-lg-2 {{ story.published | date: 'MMM d' }}
         .col-lg-7
             a(href="#") {{ story.name }}
         .col-lg-3
             a(href="#") {{ story.author }}

